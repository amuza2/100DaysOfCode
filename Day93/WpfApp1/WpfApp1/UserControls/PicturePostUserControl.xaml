<UserControl
    x:Class="WpfApp1.UserControls.PicturePostUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfApp1.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <UserControl MouseDoubleClick="UserControl_MouseDoubleClick">
                <Image x:Name="imgPost1" Source="pack://application:,,,/Images/Tipaza1.jpeg" />
            </UserControl>
            <local:PostOperationsUserControle x:Name="UCPostOperation" Height="32" />
            <local:PostLikedByUserControl Height="30" />
        </StackPanel>
    </Grid>
</UserControl>
