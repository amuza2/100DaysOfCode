<Page
    x:Class="WpfApp1.Pages.Page3"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfApp1.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Page3"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <StackPanel>
            <Label Content="This is Page 3!" />
            <Button
                Name="btnGoPageToPageOne"
                Width="100"
                Height="40"
                Click="btnGoPageToPageOne_Click"
                Content="Go to page 1"
                FontSize="16" />
            <Button
                Name="btnGoPageToPageTwo"
                Width="100"
                Height="40"
                Click="btnGoPageToPageTwo_Click"
                Content="Go to page 2"
                FontSize="16" />
        </StackPanel>
    </Grid>
</Page>
